
<h2 class="mb-4 mt-3 ml-5">ジャンル編集</h2>
<div class="container mt-2">
  <div class="row justify-content-center">
    <div class="col-md-8">
     <h4>ジャンル名</h4>
          <%= form_with model: @genre, url: admin_genre_path, method: :patch do |f| %>
            <%= f.text_field :name %>
            <%= f.submit '変更を保存', class: "btn btn-success" %>
          <% end %>
      </div>
    </div>
  </div>